<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <h1>All Languages (<%= @kinds.length %>)</h1>
    <br>
    <% @kinds.each do |kind| %>
    <%= link_to kind.language, library_show_path(kind) %>
    (<%= kind.snippets.where("user_id=?", current_user).length %> Snippets)
    <hr>
    <% end %>
  </div>
</div>
